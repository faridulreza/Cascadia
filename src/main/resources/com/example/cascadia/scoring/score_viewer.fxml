<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.cascadia.scoring.ScoreViewerController"
            minHeight="0.0" minWidth="0.0" prefHeight="350.0" prefWidth="550.0"
>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>

    <TableView fx:id="scoreTable" layoutX="14.0" layoutY="14.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columns>
            <TableColumn fx:id="scoreType"  text="Score Type"/>
            <TableColumn fx:id="player1Column"  text="Player 1"/>
            <TableColumn fx:id="Player2Column" text="Player 2"/>
            <TableColumn fx:id="Description"  text="Description"/>
        </columns>
    </TableView>

</AnchorPane>
